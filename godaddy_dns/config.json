{
  "name": "GoDaddy DNS",
  "version": "1.0.2",
  "slug": "godaddy_dns",
  "description": "GoDaddy DNS service with Let's Encrypt support",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "services",
  "map": ["ssl:rw"],
  "options": {
    "lets_encrypt": {
      "accept_terms": false,
      "certfile": "fullchain.pem",
      "keyfile": "privkey.pem",
      "renewal_period": 86400,
      "dns_delay": 60
    },
    "key": null,
    "secret": null,
    "domain": null,
    "hostname": null,
    "ttl": 600,
    "scan_interval": 300
  },
  "schema": {
    "lets_encrypt": {
      "accept_terms": "bool",
      "certfile": "str",
      "keyfile": "str",
      "renewal_period": "int",
      "dns_delay": "int"
    },
    "ipv4": "str?",
    "ipv6": "str?",
    "key": "str",
    "secret": "str",
    "domain": "str",
    "hostname": "str",
    "ttl": "int",
    "scan_interval": "int"
  }
}
